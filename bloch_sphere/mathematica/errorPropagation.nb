Cell[TextData[{
 "A qubit ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["M", "TI"], 
        StyleBox["q", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "$M_q$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b1f0d1ae-5ab4-429f-89e3-b703255830ab"],
 " that is given in matrix form can be converted to a cartesian vector ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         OverscriptBox[
          StyleBox["q", "TI"], "\[RightVector]"], "\[LongEqual]", 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["q", "TI"], "1"], ",", 
             SubscriptBox[
              StyleBox["q", "TI"], "2"], ",", 
             SubscriptBox[
              StyleBox["q", "TI"], "3"]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "$\\vec{q}=(q_1,q_2,q_3)$", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b40bf0bf-ae45-40df-a60e-6d7666e4d1ba"],
 " as follows:\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         OverscriptBox[
          StyleBox["q", "TI"], "\[RightVector]"], "\[CenterDot]", 
         OverscriptBox["\[Sigma]", "\[RightVector]"], "\[LongEqual]", 
         FractionBox["1", "2"], 
         RowBox[{"(", 
           RowBox[{
             RowBox[{"(", 
               RowBox[{
                 SubscriptBox[
                  StyleBox["q", "TI"], "1"], "+", 
                 StyleBox["i", "TI"], 
                 SubscriptBox[
                  StyleBox["q", "TI"], "2"]}], ")"}], 
             RowBox[{"(", 
               RowBox[{
                 SubscriptBox["\[Sigma]", "1"], "-", 
                 StyleBox["i", "TI"], 
                 SubscriptBox["\[Sigma]", "2"]}], ")"}], "+", 
             RowBox[{"(", 
               RowBox[{
                 SubscriptBox[
                  StyleBox["q", "TI"], "1"], "-", 
                 StyleBox["i", "TI"], 
                 SubscriptBox[
                  StyleBox["q", "TI"], "2"]}], ")"}], 
             RowBox[{"(", 
               RowBox[{
                 SubscriptBox["\[Sigma]", "1"], "+", 
                 StyleBox["i", "TI"], 
                 SubscriptBox["\[Sigma]", "2"]}], ")"}], "+", 
             SubscriptBox[
              StyleBox["q", "TI"], "3"], 
             SubscriptBox["\[Sigma]", "3"]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\vec{q}\\cdot\\vec{\\sigma}&=\\frac{1}{2}\\left(\\left(q_1+iq_2\\right)\
\\left(\\sigma_1-i\\sigma_2\\right)+\\left(q_1-iq_2\\right)\\left(\\sigma_1+i\
\\sigma_2\\right)+q_3\\sigma_3\\right)", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5d2ea1db-9972-4624-9bea-d47858aced84"],
 "\n=",
 Cell[BoxData[
  RowBox[{"(", GridBox[{
     {
      TemplateBox[<|"boxes" -> FormBox[
          SubscriptBox[
           StyleBox["q", "TI"], "3"], TraditionalForm], "errors" -> {}, 
        "input" -> "q_3", "state" -> "Boxes"|>,
       "TeXAssistantTemplate"], 
      TemplateBox[<|"boxes" -> FormBox[
          RowBox[{
            SubscriptBox[
             StyleBox["q", "TI"], "1"], "-", 
            StyleBox["i", "TI"], 
            SubscriptBox[
             StyleBox["q", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
        "input" -> "q_1-iq_2", "state" -> "Boxes"|>,
       "TeXAssistantTemplate"]},
     {
      TemplateBox[<|"boxes" -> FormBox[
          RowBox[{
            SubscriptBox[
             StyleBox["q", "TI"], "1"], "+", 
            StyleBox["i", "TI"], 
            SubscriptBox[
             StyleBox["q", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
        "input" -> "q_1+iq_2", "state" -> "Boxes"|>,
       "TeXAssistantTemplate"], 
      TemplateBox[<|"boxes" -> FormBox[
          RowBox[{"-", 
            SubscriptBox[
             StyleBox["q", "TI"], "3"]}], TraditionalForm], "errors" -> {}, 
        "input" -> "-q_3", "state" -> "Boxes"|>,
       "TeXAssistantTemplate"]}
    }], ")"}]],ExpressionUUID->"3db0c781-1210-490d-9db2-75344410c4f2"],
 "\nWe use this behavior for implementing the conversion function ",
 StyleBox["qubitmatrixToCartesian",
  FontWeight->"Bold"],
 "."
}], "Text",
