 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"rnSU2euler", "[", 
     RowBox[{"vec_", ",", "rx_", ",", "ry_", ",", "rz_"}], "]"}], ":=", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"sphericalVec", " ", "=", " ", 
       RowBox[{"ToSphericalCoordinates", "[", "vec", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Theta]", "=", 
       RowBox[{"sphericalVec", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[Phi]", "=", 
       RowBox[{"sphericalVec", "[", 
        RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"sx", "=", 
       RowBox[{"PauliMatrix", "[", "1", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"sy", "=", 
       RowBox[{"PauliMatrix", "[", "2", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"sz", "=", 
       RowBox[{"PauliMatrix", "[", "3", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Mq", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "*", 
         RowBox[{"Cos", "[", "\[Phi]", "]"}], "*", "sx"}], "+", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "*", 
         RowBox[{"Sin", "[", "\[Phi]", "]"}], "*", "sy"}], "+", 
        RowBox[{
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "*", "sz"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Un", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "I"}], "*", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"rx", "+", "rz"}], ")"}], "/", "2"}]}], "]"}], "*", 
            RowBox[{"Cos", "[", 
             RowBox[{"ry", "/", "2"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"Exp", "[", 
              RowBox[{
               RowBox[{"-", "I"}], "*", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"rx", "-", "rz"}], ")"}], "/", "2"}]}], "]"}]}], "*", 
            RowBox[{"Sin", "[", 
             RowBox[{"ry", "/", "2"}], "]"}]}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"I", "*", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"rx", "-", "rz"}], ")"}], "/", "2"}]}], "]"}], "*", 
            RowBox[{"Sin", "[", 
             RowBox[{"ry", "/", "2"}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"Exp", "[", 
             RowBox[{"I", "*", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"rx", "+", "rz"}], ")"}], "/", "2"}]}], "]"}], "*", 
            RowBox[{"Cos", "[", 
             RowBox[{"ry", "/", "2"}], "]"}]}]}], "}"}]}], "}"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", " ", "[", 
       RowBox[{"Un", ".", "Mq", ".", 
        RowBox[{"ConjugateTranspose", "[", "Un", "]"}]}], "]"}], ";"}], 
     "\[IndentingNewLine]", ")"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"rotateVector", "[", 
